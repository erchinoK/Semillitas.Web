<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("styles", required: false)

</head>
<body>
    <div class="navbar navbar-static-top navbar-inverse hidden-xs">
        <div class="container">
            <div class="navbar-collapse2 collapse2">
                <ul class="nav navbar-nav navbar-right">
                    <li>
                        <a href="@Url.Action("Index", "Home")">
                            <img alt="Espanol" src="~/Content/Images/Spain-48.png" style="height: 25px">
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index", "Home")">
                            <img alt="English" src="~/Content/Images/Great Britain-48.png" style="height: 25px">
                        </a>
                    </li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>

    <div class="navbar navbar-static-top navbar-default">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@Url.Action("Index", "Home")">
                    <img alt="Semillitas" src="~/Content/Images/Semillitas.png" class="hidden-xs" style="height: 50px;">
                    <img alt="Semillitas" src="~/Content/Images/Semillitas.png" class="visible-xs-block" style="width: 200px;">
                </a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="navbar-nav-1">@Html.ActionLink("Inicio", "Index", "Home")</li>
                    <li class="navbar-nav-2">@Html.ActionLink("Quienes Somos", "About", "Home")</li>
                    <li class="dropdown navbar-nav-3">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" 
                           aria-haspopup="true" aria-expanded="false">Planes <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Membresia Anual", "Index", "Home")</li>    
                            <li>@Html.ActionLink("Plan 1 Mes", "Index", "Home")</li>    
                            <li>@Html.ActionLink("Pase Diario", "Index", "Home")</li>    
                        </ul>
                    </li>
                    <li class="dropdown navbar-nav-4">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button"
                           aria-haspopup="true" aria-expanded="false">De Interes <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Yoga", "Index", "Home")</li>
                            <li role="separator" class="divider"></li>
                            <li>@Html.ActionLink("Fiestas y Eventos", "Index", "Home")</li>
                        </ul>
                    </li>
                    <li class="navbar-nav-5">@Html.ActionLink("Contacto", "Contact", "Home")</li>
                </ul>
                <div class="visible-xs">
                    @Html.Partial("_LoginPartial")
                    <ul class="nav navbar-nav navbar-right">
                        <li>
                            <a href="@Url.Action("Index", "Home")">
                                <img alt="Espanol" src="~/Content/Images/Spain-48.png" style="height: 25px">
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "Home")">
                                <img alt="English" src="~/Content/Images/Great Britain-48.png" style="height: 25px">
                            </a>
                        </li>
                    </ul>
                </div>                
            </div>
        </div>
    </div>

    @RenderSection("fullwidth", required: false)
    
    <div class="container-fluid body-content">
        <h1>@ViewBag.Message</h1>
        @RenderBody()
        <footer>
            
            <div class="row">

                <hr />

                <div class="col-md-4">
                    <h2>Ubicanos</h2>

                    <div class="media">
                        <div class="media-left">
                            <img class="media-object" src="data:image/svg+xml;base64," alt="...">
                        </div>
                        <div class="media-body">
                            <address>
                                <strong>Semillitas</strong><br>
                                8 Whitworth Terrace. Dublin 3<br>
                                Dublin, Ireland<br>
                            </address>
                        </div>
                    </div>                   
                    
                    <hr />

                    <div class="media">
                        <div class="media-left">
                            <img class="media-object" src="data:image/svg+xml;base64," alt="...">
                        </div>
                        <div class="media-body media-middle">
                            <p>+353 86 390 98 43</p>
                            <p>+353 86 123 45 67</p>
                        </div>
                    </div>                   
                    
                    <hr />

                    <div class="media">
                        <div class="media-left">
                            <img class="media-object" src="data:image/svg+xml;base64," alt="...">
                        </div>
                        <div class="media-body media-middle">
                            <p>hola@semillitas.com</p>
                        </div>
                    </div>
                    
                    <hr />

                    <p>
                        Siguenos por: 
                        <a href="http://www.facebook.com" class="btn-social" target="_blank"><img alt="Facebook" src="~/Content/Images/Facebook-48.png" /></a>
                        <a href="http://www.instagram.com" class="btn-social" target="_blank"><img alt="Instagram" src="~/Content/Images/Instagram-48.png" /></a>
                        <a href="http://www.twitter.com" class="btn-social" target="_blank"><img alt="Twitter" src="~/Content/Images/Twitter-48.png" /></a>
                    </p>

                </div>
                <div class="col-md-4">
                    <h2>Horario</h2>
                    <p class="pull-left">Lunes:</p>
                    <p class="pull-right">08:00am - 06:00pm</p>
                    <div class="clearfix"></div>
                    <p class="pull-left">Martes:</p>
                    <p class="pull-right">08:00am - 06:00pm</p>
                    <div class="clearfix"></div>
                    <p class="pull-left">Miercoles:</p>
                    <p class="pull-right">08:00am - 06:00pm</p>
                    <div class="clearfix"></div>
                    <p class="pull-left">Jueves:</p>
                    <p class="pull-right">08:00am - 06:00pm</p>
                    <div class="clearfix"></div>
                    <p class="pull-left">Viernes:</p>
                    <p class="pull-right">08:00am - 06:00pm</p>
                    <div class="clearfix"></div>
                    <p class="pull-left">Sabado:</p>
                    <p class="pull-right">08:00am - 06:00pm</p>
                    <div class="clearfix"></div>
                    <p class="pull-left">Domingo:</p>
                    <p class="pull-right">08:00am - 06:00pm</p>
                    <div class="clearfix"></div>
                </div>
                <div class="col-md-4">
                    <h2>Mapa</h2>
                    <div id="googleMap" style="width:100%;height:400px;"></div>
                </div>
            </div>
            <div class="footer-bottom">
                <p class="text-center"><small>&copy; @DateTime.Now.Year - Semillitas</small></p>
                <p class="text-center"><small>Site designed by <a href="http://www.polygonwd.com" target="_blank">Polygon Web Design</a>. Icons by <a href="http://www.icons8.com" target="_blank">Icons8</a></small></p>
            </div>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    <script>
        function myMap() {
            var mapProp= {
                center:new google.maps.LatLng(51.508742,-0.120850),
                zoom:5,
            };
            var map = new google.maps.Map(document.getElementById("googleMap"), mapProp);

            // Map inside Contact page
            var mapPropZoomIn = {
                center: new google.maps.LatLng(51.508742, -0.120850),
                zoom: 15,
            };
            try {
                var mapZoomIn = new google.maps.Map(document.getElementById("googleMapZoomIn"), mapPropZoomIn);
            } catch (err) { /*Not in the Contact page*/ }            
        }
    </script> 
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCCyDVf5H8vzovSYPYPhs_hQlxJkODjDrw &callback=myMap"></script>

</body>
</html>
